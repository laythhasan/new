@model Bisnicity.Models.OurserviceModels
@{
    var mess = TempData["messsages"] ?? string.Empty;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>BusnisCity Our Services</title>
    <link rel="icon" href="assets/images/logo-businicity.png" type="image/x-icon">
    <link href="~/css/toastr.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>

<body style="background-image: url(~/wwwroot/assets/images/home.jpg);">

    <section class="h-100">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col-md-10">
                    <div class="card card-registration my-4">
                        <div class="row g-0">

                            <div class="col-xl-12">
                                <div class="card-body p-md-5 text-black">
                                    <nav aria-label="breadcrumb" class="main-breadcrumb">
                                        <ol class="breadcrumb">
                                            <li class="breadcrumb-item">
                                                <a class="text-dark"
                                                   href=@Url.Action("Index","E_Business")>Show Category Company</a>
                                            </li>
                                            <li class="breadcrumb-item active">
                                                <a class="text-danger">Our Services</a>
                                            </li>

                                        </ol>
                                    </nav>
                                    <h3 class="mb-5 text-uppercase  text-center">Our Services</h3>
                                    <form asp-action="Ourservice" asp-controller="Business" enctype="multipart/form-data" method="post">
                                        @Html.HiddenFor(x => x.ChoesePrice)
                                        <div class="row">
                                            <div class="col-md-6 mb-4">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1m">City</label>
                                                    <input type="text" asp-for="City"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-4">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1n">Country</label>
                                                    <input type="text" asp-for="Country"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6 mb-2">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1m1">
                                                        WhatsApp
                                                        Link
                                                    </label>
                                                    <input type="url" asp-for="LinkWats"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1n1">
                                                        FaceBook
                                                        Link
                                                    </label>
                                                    <input type="url" asp-for="LinkFace"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1n">
                                                        Twitter
                                                        Link
                                                    </label>
                                                    <input type="url" asp-for="LinkTwet"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <div class="form-outline">
                                                    <label class="form-label" for="form3Example1n">
                                                        Instagram
                                                        Link
                                                    </label>
                                                    <input type="url" asp-for="LinkInsta"
                                                           class="form-control form-control-lg" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">GoogleMap Link</label>

                                            <input type="url" asp-for="GoogleMapLink"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">Company Number</label>

                                            <input type="tel" asp-for="CompanyNo"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">About Services</label>

                                            <input type="text" asp-for="AbuotServ"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">About Us</label>

                                            <input type="text" asp-for="AboutUs"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">Domain Name</label>

                                            <input type="text" asp-for="DomainName"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">lOGO</label>

                                            <input type="file" asp-for="File"
                                                   class="form-control form-control-lg" />
                                        </div>
                                        <div class="form-outline mb-4">
                                            <label class="form-label" for="form3Example8">Background Image</label>

                                            <input type="file" asp-for="background"
                                                   class="form-control form-control-lg" />
                                        </div>

                                        <div class="col-md-12">
                                            <label class="form-label" for="form3Example8">Choose price</label>
                                            <div class="form-floating">
                                                <select id="dd" class="form-control" asp-for="@Model.ChoesePrice" name="Cohose price">
                                                    <option value="One Month">One Month 25$</option>
                                                    <option value="Sixth Month">Sixth Month 70$</option>
                                                    <option value="One Years 100$">One Years 100$</option>
                                                </select>
                                            </div>
                                        </div>
                                        <input type="submit" class="btn btn-lg text-white mt-4"
                                               style="background-color: #178b90;" value="Save" />


                                    </form>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.9.2/dist/umd/popper.min.js"
            integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
            crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
            integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
            crossorigin="anonymous"></script>
</body>

</html>

<script src="~/js/script.js"></script>
<script>
    $("#dd").change(function () {
        $("#ChoesePrice").val($("#dd").val());
        /*alert( $("#ChoesePrice").val($("#dd").val()));*/
    });
</script>

<script src="~/lib/jquery/jquery.js"></script>
<script src="~/js/toastr.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var ss = '@mess';
        if (ss) {
            toastr.error(ss);
        }
    })
</script>