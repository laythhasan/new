@model BisnicityApp.Entites.Admin
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}

<div class="page-heading container-fluid">
    <div class="page-title">
        <div class="row">
            <div class="col-12 col-md-6 order-md-1 order-last">
                <h3>Add Admin Page</h3>
            </div>
        </div>
    </div>
    <section class="section">
        <form class="row g-3 needs-validation" asp-action="Addadmin" asp-controller="AdminB" enctype="multipart/form-data" method="post" novalidate>
            <div class="col-md-8">
                <label class="form-label">UserName</label>
                <input type="text"  asp-for="@Model.UsernameAdmin"  class="form-control" />
            </div>
            <div class="col-md-8">
                <label for="validationCustom02" class="form-label">Password</label>
                <input type="text"  asp-for="@Model.Password" class="form-control">
            </div>
            <div class="col-12">
                <button class="btn btn-success" type="submit">Add Admin</button>
            </div>
        </form>

    </section>
</div>
