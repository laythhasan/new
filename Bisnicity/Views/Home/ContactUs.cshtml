                    <a class="text-white fs-5" style="text-decoration: none;" href=@Url.Action("IndexAR","Home")>|AR </a>

                </div>
            </div>
        </div>
    </nav>

    <section class="contact d-flex align-items-center py-5" id="contact" style="margin-top:7%">
        <div class="container text-dark">
            <div class="row">
                <div class="col-md-6 d-flex justify-content-center justify-content-lg-end align-items-center px-lg-5"
                     data-aos="fade-right">
                    <div style="width:90%">
                        <div class="text-center text-lg-start py-4 pt-lg-0">
                            <h1>CONTACT US</h1>
                            <h4 class="py-2">Send To BusinessCity</h4>
                        </div>
                        <form asp-action="ContactUs" asp-controller="Home" enctype="multipart/form-data" method="post">
                            <div>
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-group py-2">
                                            <input asp-for="FirstName" type="text" class="form-control form-control-input"
                                                   placeholder="Enter name">
                                            <span asp-validation-for="@Model.FirstName" class="text-danger"></span>

                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group py-2">
                                            <input type="email" asp-for="Email" class="form-control form-control-input"
                                                   placeholder="Enter ">
                                            <span asp-validation-for="@Model.Email" class="text-danger"></span>

                                        </div>
                                    </div>
                                </div>
                                <div class="form-group py-1">
                                    <input type="tel" asp-for="PhoneNo" class="form-control form-control-input"
                                           placeholder="Enter phone number">
                                    <span asp-validation-for="@Model.PhoneNo" class="text-danger"></span>

                                </div>
                                <div class="form-group py-2">
                                    <textarea class="form-control form-control-input" asp-for="Messsage"
                                              rows="6" placeholder="Message"></textarea>
                                    <span asp-validation-for="@Model.Messsage" class="text-danger"></span>

                                </div>
                            </div>
                            <div class="my-3">
                                <input class="btn text-white" type="submit" value="Send" style="background-color: #0c6e78;" href="#your-link" />
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-6 d-flex align-items-center" data-aos="fade-down">

                    <iframe src="https://maps.google.com/maps?width=600&amp;height=400&amp;hl=en&amp;q=Businicity _ مدينة الأعمال&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"
                            frameborder="0" style="border:0; width: 100%; height: 100%;" allowfullscreen></iframe>

                </div>
            </div>
        </div>
    </section>

    <footer class="bg-dark p-4 text-white text-center" style="background-image: url(../assets/images/footer.png);">
        <div style="margin-bottom: 1%;">
            <a class="text-white" style="text-decoration: none; padding: 1%;" href=@Url.Action("Index1","Home")>
